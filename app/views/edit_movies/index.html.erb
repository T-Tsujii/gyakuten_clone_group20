<div class="mt-5">
  <div class="text-center">
    <% @edit_movies.each do |edit_movie|  %>
      <p style="font-size: 20px"><%= "#{edit_movie.title}" %></p>
      <iframe width="560" height="315" src="<%= edit_movie.url %> " frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen="allowfullscreen" "></iframe>
    <% end %>
  </div>
</div>

<div class="mt-5">
  <div align="center">
    <div class="php_movie">
      <% @monies.each do |money| %>
        <p style ="font-size: 20px"><%= "#{money.title}" %></p>
        <iframe width="560" height="315" src="<%= money.url %>" frameborder="0" allow="accelerometer; autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
      <% end %>
    </div>
  </div>
</div>
